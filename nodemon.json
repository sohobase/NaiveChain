{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "src/common/terminal/**",
    "store/**.json"
  ],
  "verbose": false,
  "execMap": {
    "js": "node --harmony"
  },
  "watch": [
    "src/**"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,json"
}
